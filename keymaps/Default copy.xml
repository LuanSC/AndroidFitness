<keymap version="1" name="Default copy" parent="$default">
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="shift insert" />
  </action>
  <action id="ActivateLogcatToolWindow">
    <keyboard-shortcut first-keystroke="alt 6" />
  </action>
  <action id="ActivateTODOToolWindow" />
  <action id="FileChooser.TogglePathShowing" />
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
    <keyboard-shortcut first-keystroke="ctrl p" />
  </action>
  <action id="GotoSuperMethod" />
  <action id="ParameterInfo" />
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="ctrl v" />
  </action>
  <action id="UnsplitAll">
    <keyboard-shortcut first-keystroke="ctrl u" />
  </action>
</keymap>